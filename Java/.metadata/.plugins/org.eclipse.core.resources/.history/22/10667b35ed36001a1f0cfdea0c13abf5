package com.mercury.collection;

public class IterationTest {
	
	public static void iterateArray() {
		int[] nums = {1, 2, 3, 4};
		
		// 1. traditional for loop
		for(int i = 0; i < nums.length; i++) {
			System.out.println(nums[i]);
		}
	}

	public static void main(String[] args) {
		iterateArray();
	}

}
